#layer2 {
    position: absolute;
    visibility: hidden;
    width: 400px;
    height: 300px;
    left: 20px;
    top: 20px;
    background-color: #ccc;
    border: 1px solid #000;
    padding: 10px;
    margin: 0px;
}
#layer1 {
    position: absolute;
    background-color: white;
    top: 100px;
    width: 80%;
    margin-left: 10%;
    margin-right: 10%;
    z-index: 10;
    padding: 10px;
    visibility: hidden;
}

#txtinput {
    width:80%;
